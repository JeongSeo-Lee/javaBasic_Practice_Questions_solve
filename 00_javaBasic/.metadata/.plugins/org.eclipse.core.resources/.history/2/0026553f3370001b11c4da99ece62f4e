package com.hihighlinux;

public class Practice44 {
	public static void main(String args[]) {
		int sum = 0;
		int k = 1;
		for(int i = 1; true; i++) {
			System.out.println(i);
			sum += i;
			if(sum >=100) {
				System.out.println(i);
				break;
			}
			if(i%2==0) {
				i = i * (-1);
			} else {
				i = i * (-1);
			}
		}
		
	}
}
